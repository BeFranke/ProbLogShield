{
  "workflow_name": "ppo",
  "raw_logger": "ppo_raw",
  "info_logger": "ppo_info",
  "env_type": "GoalFinding-v0",

  "env_features": {
    "layout": "grid7x7_5_ghosts",
    "reward_goal": 10,
    "reward_crash": 0,
    "reward_food": 0,
    "reward_time": -0.1,
    "render": false,
    "max_steps": 200,
    "num_maps": 100,
    "seed": 567,
    "render_mode": "gray"
  },

  "model_features": {
    "name": "no_shielding",
    "encoder_params": {
      "height": 240,
      "width": 240,
      "downsampling_size": 7
    },
    "params": {
      "log_interval": 1,
      "batch_size": 128,
      "n_epochs": 40,
      "n_steps": 1024,
      "learning_rate": 0.001,
      "seed": 567,
      "clip_range": 0.2,
      "gamma": 0.99,
      "step_limit": 1000,
      "program_type": "relative_loc_simple",
      "debug_program_type": "relative_loc_simple",
      "alpha": 0,
      "n_ghost_locs": 4,
      "differentiable_shield": true,
      "net_arch_shared": [],
      "net_arch_pi": [128],
      "net_arch_vf": [128],
      "sensor_noise": 0,
      "max_num_rejected_samples": 100000,
      "use_learned_observations": null,
      "noisy_observations": null,
      "observation_type": null
    }
  }
}


