{
  "workflow_name": "ppo",
  "raw_logger": "ppo_raw",
  "info_logger": "ppo_info",
  "env_type": "Boxoban-Train-v0",

  "env_features": {
    "max_steps": 120,
    "penalty_for_step": -0.1,
    "penalty_box_off_target": -1,
    "reward_box_on_target": 1,
    "reward_finished": 10,
    "reward_last": 0,
    "dim_room": [7,7],
    "num_boxes": 2,
    "render": false,
    "render_mode": "tiny_rgb_array",
    "action_size": 5,
    "difficulty": "medium",
    "split": "2box10map"
  },
  "model_features": {
    "params": {
      "log_interval": 1,
      "batch_size": 512,
      "n_epochs": 40,
      "n_steps": 2048,
      "learning_rate": 0.001,
      "seed": 567,
      "clip_range": 0.2,
      "gamma": 0.99,
      "step_limit": 5000000,
      "program_type": "sokoban_corner2",
      "debug_program_type": "sokoban_corner2",
      "alpha": 0.1,
      "n_box_locs": 4,
      "n_corner_locs": 4,
      "differentiable_shield": true,
      "net_arch_shared": [],
      "net_arch_pi": [128],
      "net_arch_vf": [128],
      "sensor_noise": 0,
      "max_num_rejected_samples": 100000
    }
  }
}
