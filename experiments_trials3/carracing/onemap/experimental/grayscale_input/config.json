{
  "workflow_name": "ppo",
  "raw_logger": "ppo_raw",
  "info_logger": "ppo_info",
  "env_type": "CarRacing-v1",

  "env_features": {
    "render": true,
    "num_maps": 1,
    "seed": 567
  },

  "model_features": {
    "name": "no_shielding",
    "params": {
      "log_interval": 1,
      "batch_size": 128,
      "n_epochs": 10,
      "n_steps": 2048,
      "learning_rate": 0.001,
      "seed": 567,
      "clip_range": 0.2,
      "gamma": 0.99,
      "step_limit": 1000000,
      "program_type": "carracing_grass",
      "debug_program_type": "carracing_grass",
      "alpha": 0,
      "n_grass_locs": 3,
      "differentiable_shield": true,
      "net_arch_shared": [],
      "net_arch_pi": [64, 64],
      "net_arch_vf": [64, 64],
      "sensor_noise": 0,
      "max_num_rejected_samples": 100000,
      "convert_input_to_grayscale": true
    }
  }
}