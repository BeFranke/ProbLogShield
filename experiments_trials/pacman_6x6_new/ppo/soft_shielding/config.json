{
  "workflow_name": "ppo",
  "raw_logger": "ppo_raw",
  "info_logger": "ppo_info",
  "env_type": "Pacman-v0",

  "env_features": {
    "layout": "grid6x6",
    "reward_goal": 10,
    "reward_crash": -10,
    "reward_food": 0,
    "reward_time": -1,
    "render": false,
    "max_steps": 20,
    "seed": 567
  },

  "model_features": {
    "name": "soft_shielding",
    "params": {
      "log_interval": 1,
      "batch_size": 128,
      "n_epochs": 40,
      "n_steps": 1024,
      "learning_rate": 0.001,
      "seed": 567,
      "clip_range": 0.2,
      "gamma": 0.99,
      "step_limit": 500000,
      "program_type": "relative_loc",
      "shield": true,
      "detect_ghosts": true,
      "detect_walls": true,
      "n_ghost_locs": 4,
      "n_wall_locs": 4,
      "net_arch_shared": [],
      "net_arch_pi": [128],
      "net_arch_vf": [128]
    }
  },

  "visualize_settings": {
    "window_size": 2000,
    "window_speed": 1000
  }

}


