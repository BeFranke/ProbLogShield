{
  "workflow_type": "pacman_ppo_dpl",
  "workflow_name": "ppo_shield",
  "raw_logger": "ppo_shield_raw",
  "info_logger": "ppo_shield_info",

  "env_features": {
    "layout": "grid5x5_3_ghosts",
    "reward_goal": 10,
    "reward_crash": -10,
    "reward_food": 0,
    "reward_time": -1,
    "seed": 567,
    "render": false
  },

  "model_features": {
    "name": "hard_shielding",
    "params": {
      "log_interval": 1,
      "batch_size": 64,
      "n_epochs": 10,
      "n_steps": 2048,
      "learning_rate": 1e-3,
      "seed": 567,
      "gamma": 0.99,
      "step_limit": 200000,
      "program_type": "relative_loc",
      "shield": true,
      "detect_ghosts": false,
      "detect_walls": false,
      "net_arch_shared": [],
      "net_arch_pi": [128],
      "net_arch_vf": [128]
    }
  },

  "visualize_settings": {
    "window_size": 2000,
    "window_speed": 1000
  }

}


