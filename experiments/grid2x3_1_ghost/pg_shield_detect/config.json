{
  "workflow_type": "pacman_pg_dpl",
  "workflow_name": "pg_shield_detect",
  "raw_logger": "pg_shield_detect_raw",
  "info_logger": "pg_shield_detect_info",

  "env_features": {
    "layout": "grid2x3",
    "reward_goal": 10,
    "reward_crash": -10,
    "reward_food": 0,
    "reward_time": -1,
    "seed": 567,
    "render": false
  },

  "model_features": {
    "name": "soft_shielding",
    "params": {
      "learning_rate": 1e-3,
      "seed": 567,
      "gamma": 0.99,
      "step_limit": 200000,
      "program_type": "relative_loc",
      "shield": true,
      "detect_ghosts": true,
      "detect_walls": true
    }
  },

  "visualize_settings": {
    "window_size": 2000,
    "window_speed": 1000
  }

}


