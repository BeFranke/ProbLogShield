{
  "workflow_type": "pacman_ppo_dpl",
  "workflow_name": "ppo_shield_detect",
  "raw_logger": "ppo_shield_detect_raw",
  "info_logger": "ppo_shield_detect_info",

  "env_features": {
    "layout": "grid5x5_5_ghosts",
    "reward_goal": 10,
    "reward_crash": -10,
    "reward_food": 0,
    "reward_time": -1,
    "seed": 567,
    "render": false
  },

  "model_features": {
    "name": "soft_shielding",
    "params": {
      "log_interval": 1,
      "batch_size": 64,
      "n_epochs": 10,
      "n_steps": 32768,
      "learning_rate": 1e-3,
      "seed": 567,
      "gamma": 0.99,
      "step_limit": 1000000,
      "program_type": "relative_loc",
      "shield": true,
      "detect_ghosts": true,
      "detect_walls": true,
      "net_arch_shared": [],
      "net_arch_pi": [256],
      "net_arch_vf": [256]
    }
  },

  "visualize_settings": {
    "window_size": 2000,
    "window_speed": 1000
  }

}


