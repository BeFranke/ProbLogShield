{
  "workflow_type": "pacman_ppo_dpl",
  "workflow_name": "ppo",
  "raw_logger": "ppo_raw",
  "info_logger": "ppo_info",
  "env_type": "Sokoban-v0",

  "env_features": {
    "num_boxes": 1,
    "max_steps": 200,
    "dim_room": [5, 5],
    "penalty_for_step": -1,
    "penalty_box_off_target": -1,
    "reward_box_on_target": 1,
    "reward_finished": 10,
    "reward_last": 0,
    "render": false,
    "render_mode": "tiny_rgb_array"
  },

  "model_features": {
    "name": "no_shielding",
    "params": {
      "log_interval": 1,
      "batch_size": 64,
      "n_epochs": 10,
      "n_steps": 128,
      "learning_rate": 1e-3,
      "seed": 567,
      "gamma": 0.99,
      "step_limit": 200000,
      "program_type": "relative_loc",
      "shield": false,
      "detect_ghosts": false,
      "detect_walls": false,
      "net_arch_shared": [],
      "net_arch_pi": [128],
      "net_arch_vf": [128]
    }
  },

  "visualize_settings": {
    "window_size": 2000,
    "window_speed": 1000
  }

}


