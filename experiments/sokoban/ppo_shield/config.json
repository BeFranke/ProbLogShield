{
  "workflow_type": "pacman_ppo_dpl",
  "workflow_name": "ppo_shield",
  "raw_logger": "ppo_shield_raw",
  "info_logger": "ppo_shield_info",
  "env_type": "Sokoban-v0",

  "env_features": {
    "num_boxes": 1,
    "max_steps": 20,
    "dim_room": [5, 5],
    "penalty_for_step": -1,
    "penalty_box_off_target": -1,
    "reward_box_on_target": 1,
    "reward_finished": 10,
    "reward_last": 0,
    "render": false,
    "render_mode": "tiny_rgb_array"
  },

  "model_features": {
    "name": "hard_shielding",
    "params": {
      "log_interval": 1,
      "batch_size": 4,
      "n_epochs": 10,
      "n_steps": 20,
      "learning_rate": 1e-4,
      "seed": 567,
      "gamma": 0.99,
      "step_limit": 40000,
      "program_type": "sokoban_relative",
      "shield": true,
      "detect_boxes": false,
      "detect_walls": false,
      "detect_targets": false,
      "net_arch_shared": [],
      "net_arch_pi": [256],
      "net_arch_vf": [256]
    }
  },

  "visualize_settings": {
    "window_size": 2000,
    "window_speed": 1000
  }

}


